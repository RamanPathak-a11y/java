import java.util.Scanner;

public class Program5 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter base : ");
        int x = sc.nextInt();
        System.out.print("Enter exponent : ");
        int y = sc.nextInt();
        int result = 1;
        for (int i = 1; i <= y; i++) {
            result *= x;
        }
        System.out.println("x^y = " + result);
    }
}
